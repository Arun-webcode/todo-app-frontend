{
    "buildCommand": "ng build --configuration production",
    "outputDirectory": "www",
    "framework": null,
    "installCommand": "npm install",
    "routes": [
        {
            "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}